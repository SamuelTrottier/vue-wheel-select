{
  "name": "vue-wheel-select",
  "version": "1.0.1",
  "description": "A simple select that you scroll through to select items.",
  "author": "Samuel Trottier",
  "license": "MIT",
  "private": false,
  "keywords": [
    "vue",
    "select",
    "wheel",
    "selection",
    "mobile"
  ],
  "repository": "https://github.com/SamuelTrottier/vue-wheel-select",
  "files": [
    "dist"
  ],
  "scripts": {
